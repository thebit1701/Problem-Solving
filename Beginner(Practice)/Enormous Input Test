#include<iostream>
using namespace std;

int main()
{
    int n , k , i=1 , A[i] , count=0;
  
    cout<<"Enter value of n and k elements";
    cin>>n>>k;
    cout<<"Enter integer numbers ";
    for(i=1;i<=n;i++)
    {
        cin>>A[i];
    }
    
    for(i=1;i<=n;i++)
    {
       if(A[i]%k==0)
       {
           count++;
        }
       else {
           count=count;
           
       }
    }
   
    cout<<" Value of n and k elements : " <<"  "<< n<< " " <<k;
    cout<<" values of n elements : " <<endl;
    for(i=1;i<=n;i++)
    {
        cout<<A[i] <<endl;
    }
    cout<<"Number of values that are  divisble by "<<" "<<k<<" is :"<<" " << count ;
return 0;
}
